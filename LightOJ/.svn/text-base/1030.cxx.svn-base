inline bool CI(int &_x) {
    return scanf("%d",&_x)==1;
}
inline bool CI(int &_x, int &_y) {
    return CI(_x)&&CI(_y) ;
}
inline bool CI(int &_x, int &_y, int &_z) {
    return CI(_x)&&CI(_y)&&CI(_z) ;
}
inline bool CI(int &_x, int &_y, int &_z, int &_zz) {
    return CI(_x)&&CI(_y)&&CI(_z)&&CI(_zz) ;
}
const int maxn=100+5;
int N;
int golds[maxn];
inline void Read() {
    CI(N);
    fo(i,N) {
        CI(golds[i]);
    }
}
double dp[maxn];
bool visit[maxn];
double go(int at) {
    if (at==N)return 0;
    if (visit[at])return dp[at];

    visit[at]=1;
    double summed=0.;
    int upto=min(at+6,N);
    int tmp=(upto-at);
    for (int i=at;i<upto;++i) {
        summed=summed+(  ( go(i+1)+golds[i] )*( (double)1./tmp) );
    }

    return dp[at]=summed;
}
inline void Proc() {
    SET(visit,false);
    double var=golds[0]+go(1);
    cout<<setprecision(6)<<fixed<<var;
    line;
}
int main() {
    int tt;
    CI(tt);
    foE(i,1,tt) {
